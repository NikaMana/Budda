.logo{
  width: 47px;
  height: 26px;
  color: #414c63;
  text-decoration: none;
  font-size: 36px;
  font-weight: 800;
  display: inline-block;
  position: relative;
  vertical-align: middle;
  z-index: 110;
  padding: 1rem 1.375rem;
  cursor: pointer;
  
  @include tablets(){
    font-size: 2.25rem;
  }
}